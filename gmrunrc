ShowDotFiles = 1
TabTimeout = 1

Terminal = gnome-terminal
TermExec = ${Terminal} -e
AlwaysInTerm = htop man info

History = 500

URL_http = x-www-browser %u
URL_info = ${Terminal} --command info %s
URL_man = ${TermExec} 'man %s'
URL_info = ${TermExec} 'info %s'
URL_file = exo-open %s
URL_f = exo-open %s
URL_g = x-www-browser 'http://www.google.com/search?q=%s'
